{
    "name": "@hexrw/spapi",
    "version": "1.0.0-alpha.5",
    "description": "Amazon Selling Partner API JavaScript client with full type coverage.",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "test": "jest",
        "test:ts": "tsc --noEmit",
        "schema:generate": "openapi-typescript --redoc ../api/redocly.yaml",
        "schema:generate-joined": "openapi-typescript ../api/sp-api.yaml -o ./src/schemas/index.ts",
        "publish": "pnpm publish --access public",
        "build": "tsc ./src/index.ts --outDir ./dist"
    },
    "keywords": [
        "spapi",
        "amazon",
        "selling",
        "partner",
        "api",
        "aws",
        "amazon selling partner api",
        "seller",
        "vendor",
        "central",
        "lwa",
        "fba",
        "typescript",
        "client",
        "sdk",
        "amazon spapi",
        "fulfillment",
        "reports"
    ],
    "author": "hexrw",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.7",
        "@types/node": "^20.10.4",
        "jest": "^29.7.0",
        "openapi-typescript": "7.0.0-next.2",
        "ts-to-zod": "^3.2.0",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "ofetch": "^1.3.3",
        "openapi-typescript-helpers": "^0.0.4",
        "ufo": "^1.3.2",
        "zod": "^3.22.4"
    }
}
